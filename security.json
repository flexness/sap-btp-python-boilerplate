{
  "xsappname": "python-tutorial",
  "tenant-mode": "dedicated",
  "oauth2-configuration": {
    "redirect-uris": [
      "https://*.cfapps.eu10.hana.ondemand.com/**"
    ]
  },
  "scopes": [
    {
      "name": "$XSAPPNAME.Viewer",
      "description": "Allows viewing data"
    }
  ],
  "role-templates": [
    {
      "name": "Viewer",
      "description": "Role template for viewing data",
      "scope-references": [
        "$XSAPPNAME.Viewer"
      ]
    }
  ]
}
